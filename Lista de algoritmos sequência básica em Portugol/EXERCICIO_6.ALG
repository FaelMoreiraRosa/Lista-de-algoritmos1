Algoritmo "Exercicio_5"

var
num:real
dobro:real
terpart:real

inicio
   
   escreval("Digite um número:")
   leia(num)

   dobro <- num * 2
   terpart <- num / 3
   
   escreval("O Número é:", num)
   escreval("Seu dobro é:", dobro)
   escreval("E sua terça-parte é:", terpart)

Fimalgoritmo